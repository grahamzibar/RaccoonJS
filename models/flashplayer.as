package {		import flash.display.Sprite;	import flash.external.ExternalInterface;	import flash.system.Security;	import coreplayer;		public class flashplayer extends Sprite {		public var player:coreplayer;				public function flashplayer() {			Security.allowDomain("*");						player = new coreplayer();			//player.playTrack("http://media.supernova.com/media1/286291/music/02dirtyclyde.mp3");			//player.playTrack("http://media.supernova.com/media1/173499/music/REMINISCE2.mp3");			trace("Created coreplayer");			/* This constructor initializes the ExternalInterface */			if (flash.external.ExternalInterface.available) {				trace("External Interface Available");				//this.getNothing				flash.external.ExternalInterface.addCallback("getNothing", player.getNothing);								//this.getTrackUrl				flash.external.ExternalInterface.addCallback("getTrackUrl", player.getTrackUrl);				//this.resume				flash.external.ExternalInterface.addCallback("resume", player.resume);				//this.pause				flash.external.ExternalInterface.addCallback("pause", player.pause);				//this.playTrack				flash.external.ExternalInterface.addCallback("playTrack", player.playTrack);				//this.loadTrack				flash.external.ExternalInterface.addCallback("loadTrack", player.loadTrack);				//this.getLoadProgress				flash.external.ExternalInterface.addCallback("getLoadProgress", player.getLoadProgress);								//this.seekTo				flash.external.ExternalInterface.addCallback("seekTo", player.seekTo);								//this.getTrackProgress				flash.external.ExternalInterface.addCallback("getTrackProgress", player.getTrackProgress);								//this.getTrackTime				flash.external.ExternalInterface.addCallback("getTrackTime", player.getTrackTime);								//this.setVolume				flash.external.ExternalInterface.addCallback("setVolume", player.setVolume);								//this.getVolume				flash.external.ExternalInterface.addCallback("getVolume", player.getVolume);								// Now we need to call the onready, dawg!				flash.external.ExternalInterface.call("window.apps.Player.onready", true);			}			trace("Complete!");		}			}	}